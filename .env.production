# ToyTown Frontend Production Environment Variables

# API Configuration
# Update with your production API URL
VITE_API_BASE_URL=https://api.toytown.example.com/api/v1

# Razorpay Configuration (Live Mode)
# Replace with your actual Razorpay live key ID from Razorpay Dashboard
VITE_RAZORPAY_KEY_ID=rzp_live_REPLACE_WITH_LIVE_KEY

# Application Configuration
VITE_APP_NAME=Aadhav's ToyTown
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_OAUTH_LOGIN=true
VITE_ENABLE_GOOGLE_LOGIN=true
VITE_ENABLE_FACEBOOK_LOGIN=true

# Analytics & Monitoring (optional)
# VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXXX
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# Performance
VITE_ENABLE_LAZY_LOADING=true
VITE_IMAGE_OPTIMIZATION=true
